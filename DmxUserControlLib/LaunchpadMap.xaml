        <Label Grid.Column="9" Grid.Row="9" Content="H" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <!--#endregion-->

        <!--#region System Button-->
        <Button Grid.Column="0" Grid.Row="1" Content="" Margin="5" Click="Button_Click" Background="Gray"/>
        <Button Grid.Column="1" Grid.Row="1" Content="" Margin="5" Click="Button_Click" Background="Gray"/>
        <Button Grid.Column="2" Grid.Row="1" Content="" Margin="5" Click="Button_Click" Background="Gray"/>
