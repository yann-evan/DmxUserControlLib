        <Label Grid.Column="2" Grid.Row="0" Content="3" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <Label Grid.Column="3" Grid.Row="0" Content="4" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <Label Grid.Column="4" Grid.Row="0" Content="5" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <Label Grid.Column="5" Grid.Row="0" Content="6" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <Label Grid.Column="6" Grid.Row="0" Content="7" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <Label Grid.Column="7" Grid.Row="0" Content="8" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <!--#endregion-->

        <!--#region Row label-->
        <Label Grid.Column="9" Grid.Row="2" Content="A" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <Label Grid.Column="9" Grid.Row="3" Content="B" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
        <Label Grid.Column="9" Grid.Row="4" Content="C" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="white"/>
